import math
x = float(input("Введіть x: "))
if x >= 5:
    a = -9 + math.log(x) + math.sqrt(x)
    print('x >= 5:',a)
elif -0.5 < x :
    b = math.cos(x) + math.sin(x)
    print('-0.5 < x :',b)
elif x <= -0.5:
    c = math.fabs(-x ** 2 + 2 * x ** -2)
    print('x <= -0.5:',c)
  
else:
    print("Обчислення виразу неможливе ")
